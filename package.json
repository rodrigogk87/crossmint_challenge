{
  "name": "crossmintchallenge",
  "version": "1.0.0",
  "description": "dev challenge for crossmint company",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "startp1": "tsc && node -r dotenv/config dist/phaseone.js",
    "devp1": "tsc && nodemon -r dotenv/config ./src/phaseone.ts -e ts",
    "startp2": "tsc && node -r dotenv/config dist/phasetwo.js",
    "devp2": "tsc && nodemon -r dotenv/config ./src/phasetwo.ts -e ts",
    "devclrmap": "tsc && nodemon -r dotenv/config ./src/clearmap.ts -e ts",
    "lint": "eslint . --ext .ts"
  },
  "author": "rodrigo garcia kosinski",
  "license": "ISC",
  "devDependencies": {
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.0",
    "pino": "^8.8.0"
  }
}